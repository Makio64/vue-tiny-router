{
  "name": "vue-tiny-router",
  "version": "1.3.2",
  "type": "module",
  "description": "Hi! I'm a minimalist & lightweight vue3 router",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "vite build",
    "size:check": "size-limit",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Makio64/vue-tiny-router.git"
  },
  "keywords": [
    "vue",
    "router",
    "component",
    "tiny"
  ],
  "author": "Makio64 | David Ronai <davronai@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Makio64/vue-tiny-router/issues"
  },
  "homepage": "https://github.com/Makio64/vue-tiny-router#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "gzip-size": "^7.0.0",
    "jsdom": "^26.1.0",
    "size-limit": "^11.2.0",
    "terser": "^5.39.2",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "size-limit": [
    {
      "name": "vue-tiny-router (ES Module)",
      "path": "dist/index.js",
      "limit": "1.2 KB",
      "gzip": true
    }
  ]
}
