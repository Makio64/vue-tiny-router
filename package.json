{
  "name": "vue-tiny-router",
  "version": "1.7.1",
  "type": "module",
  "description": "Hi! I'm a minimalist & lightweight vue3 router",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./index.d.ts"
    }
  },
  "types": "index.d.ts",
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "test": "vitest run",
    "build": "vite build",
    "size:check": "size-limit",
    "prepublishOnly": "pnpm run build",
    "example:dev": "cd example && pnpm run dev",
    "example:build": "cd example && pnpm run build",
    "example:preview": "cd example && pnpm run preview",
    "docs:dev": "cd docs && pnpm run docs:dev",
    "docs:build": "cd docs && pnpm run docs:build",
    "docs:preview": "cd docs && pnpm run docs:preview",
    "dev": "pnpm run example:dev",
    "start": "pnpm run example:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Makio64/vue-tiny-router.git"
  },
  "keywords": [
    "vue",
    "router",
    "component",
    "tiny"
  ],
  "author": "Makio64 | David Ronai <davronai@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Makio64/vue-tiny-router/issues"
  },
  "homepage": "https://github.com/Makio64/vue-tiny-router#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.5.1",
    "gzip-size": "^7.0.0",
    "jsdom": "^27.1.0",
    "size-limit": "^11.2.0",
    "terser": "^5.44.1",
    "vite": "^7.2.2",
    "vitest": "^4.0.8",
    "vue-eslint-parser": "^10.2.0"
  },
  "size-limit": [
    {
      "name": "vue-tiny-router (ES Module)",
      "path": "dist/index.js",
      "limit": "1.4 KB",
      "brotli": true
    }
  ]
}
